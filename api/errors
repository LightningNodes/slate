    <tr>
      <td>2303</td>
      <td>400</td>
      <td>Contract pair not found</td>
      <td><code>v1/exchange/update/leverage</code></td>
    </tr>
    <tr>
      <td>2304</td>
      <td>400</td>
      <td>Leverage value out of range</td>
      <td><code>v1/exchange/update/leverage</code></td>
    </tr>
